package com.revature.beans;

public class employee {
	
	private int appID;
	private int empID;
	private int appCodeOne;
	private int appCodeTwo;
	private int appCodeThree;
	private int amount;
	private boolean approval;
	
	public employee() {
		super();
	}

	public employee(int appID, int empID, int appCodeOne, int appCodeTwo, int appCodeThree, int amount,
			boolean approval) {
		super();
		this.appID = appID;
		this.empID = empID;
		this.appCodeOne = appCodeOne;
		this.appCodeTwo = appCodeTwo;
		this.appCodeThree = appCodeThree;
		this.amount = amount;
		this.approval = approval;
	}

	public int getAppID() {
		return appID;
	}

	public void setAppID(int appID) {
		this.appID = appID;
	}

	public int getEmpID() {
		return empID;
	}

	public void setEmpID(int empID) {
		this.empID = empID;
	}

	public int getAppCodeOne() {
		return appCodeOne;
	}

	public void setAppCodeOne(int appCodeOne) {
		this.appCodeOne = appCodeOne;
	}

	public int getAppCodeTwo() {
		return appCodeTwo;
	}

	public void setAppCodeTwo(int appCodeTwo) {
		this.appCodeTwo = appCodeTwo;
	}

	public int getAppCodeThree() {
		return appCodeThree;
	}

	public void setAppCodeThree(int appCodeThree) {
		this.appCodeThree = appCodeThree;
	}

	public int getAmount() {
		return amount;
	}

	public void setAmount(int amount) {
		this.amount = amount;
	}

	public boolean isApproval() {
		return approval;
	}

	public void setApproval(boolean approval) {
		this.approval = approval;
	}

	@Override
	public String toString() {
		return "employee [appID=" + appID + ", empID=" + empID + ", appCodeOne=" + appCodeOne + ", appCodeTwo="
				+ appCodeTwo + ", appCodeThree=" + appCodeThree + ", amount=" + amount + ", approval=" + approval + "]";
	}
}
