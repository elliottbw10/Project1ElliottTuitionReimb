package com.revature.beans;

import java.sql.Date;

public class empForm {
	
	private String uName;
	private String fName;
	private String lName;
	private String classType;
	private int classCost;
	private String classGrade;
	private String descript;
	private Date classStart;
	private int formNum;
	private int appCode1;
	private int appCode2;
	private int appCode3;
	
	public empForm() {
		super();
		// TODO Auto-generated constructor stub
	}

	public empForm(String uName, String fName, String lName, String classType, int classCost, String classGrade,
			String descript, Date classStart, int formNum, int appCode1, int appCode2, int appCode3) {
		super();
		this.uName = uName;
		this.fName = fName;
		this.lName = lName;
		this.classType = classType;
		this.classCost = classCost;
		this.classGrade = classGrade;
		this.descript = descript;
		this.classStart = classStart;
		this.formNum = formNum;
		this.appCode1 = appCode1;
		this.appCode2 = appCode2;
		this.appCode3 = appCode3;
	}

	public empForm(String string, String string2, String string3, int int1, int int2, int int3, int int4, String uName, String fName, String lName, int formNum, int appCode2, int appCode1, int appCode3) {
		super();
		this.uName = uName;
		this.fName = fName;
		this.lName = lName;
		this.formNum = formNum;
		this.appCode1 = appCode1;
		this.appCode2 = appCode2;
		this.appCode3 = appCode3;
	}

	public empForm(String string, String string2, String string3, int int1, int int2, int int3, int int4) {
		// TODO Auto-generated constructor stub
	}

	public String getuName() {
		return uName;
	}

	public void setuName(String uName) {
		this.uName = uName;
	}

	public String getfName() {
		return fName;
	}

	public void setfName(String fName) {
		this.fName = fName;
	}

	public String getlName() {
		return lName;
	}

	public void setlName(String lName) {
		this.lName = lName;
	}

	public String getClassType() {
		return classType;
	}

	public void setClassType(String classType) {
		this.classType = classType;
	}

	public int getClassCost() {
		return classCost;
	}

	public void setClassCost(int classCost) {
		this.classCost = classCost;
	}

	public String getClassGrade() {
		return classGrade;
	}

	public void setClassGrade(String classGrade) {
		this.classGrade = classGrade;
	}

	public String getDescript() {
		return descript;
	}

	public void setDescript(String descript) {
		this.descript = descript;
	}

	public Date getClassStart() {
		return classStart;
	}

	public void setClassStart(Date classStart) {
		this.classStart = classStart;
	}

	public int getFormNum() {
		return formNum;
	}

	public void setFormNum(int formNum) {
		this.formNum = formNum;
	}

	public int getAppCode1() {
		return appCode1;
	}

	public void setAppCode1(int appCode1) {
		this.appCode1 = appCode1;
	}

	public int getAppCode2() {
		return appCode2;
	}

	public void setAppCode2(int appCode2) {
		this.appCode2 = appCode2;
	}

	public int getAppCode3() {
		return appCode3;
	}

	public void setAppCode3(int appCode3) {
		this.appCode3 = appCode3;
	}

	@Override
	public String toString() {
		return "empForm [uName=" + uName + ", fName=" + fName + ", lName=" + lName + ", classType=" + classType
				+ ", classCost=" + classCost + ", classGrade=" + classGrade + ", descript=" + descript + ", classStart="
				+ classStart + ", formNum=" + formNum + ", appCode1=" + appCode1 + ", appCode2=" + appCode2
				+ ", appCode3=" + appCode3 + "]";
	}
}
